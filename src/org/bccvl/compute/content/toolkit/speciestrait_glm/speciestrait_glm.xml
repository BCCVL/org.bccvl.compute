<model xmlns:security="http://namespaces.plone.org/supermodel/security"
       xmlns:marshal="http://namespaces.plone.org/supermodel/marshal"
       xmlns:form="http://namespaces.plone.org/supermodel/form"
       xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema based-on="zope.interface.Interface">
    <field name="family" type="zope.schema.Choice">
      <title>Family</title>
      <description>Please refer to the R documention</description>
      <values>
        <element>binomial</element>
        <element>binomial(link="logit")</element>
        <element>binomial(link="probit")</element>
        <element>binomial(link="cauchit")</element>
        <element>binomial(link="log")</element>
        <element>binomial(link="cloglog")</element>
        <element>gaussian</element>
        <element>gaussian(link="identity")</element>
        <element>gaussian(link="log")</element>
        <element>gaussian(link="inverse")</element>
        <element>Gamma</element>
        <element>Gamma(link="inverse")</element>
        <element>Gamma(link="identity")</element>
        <element>Gamma(link="log")</element>
        <element>poisson</element>
        <element>poisson(link="log")</element>
        <element>poisson(link="identity")</element>
        <element>poisson(link="sqrt")</element>
        <element>inverse.gaussian</element>
        <element>inverse.gaussian(link="1/mu^2")</element>
        <element>inverse.gaussian(link="inverse")</element>
        <element>inverse.gaussian(link="identity")</element>
        <element>inverse.gaussian(link="log")</element>
        <element>quasi</element>
        <element>quasi(link="logit")</element>
        <element>quasi(link="probit")</element>
        <element>quasi(link="cloglog")</element>
        <element>quasi(link="identity")</element>
        <element>quasi(link="inverse")</element>
        <element>quasi(link="log")</element>
        <element>quasi(link="1/mu^2")</element>
        <element>quasi(link="sqrt")</element>
        <element>quasibinomial</element>
        <element>quasibinomial(link="logit")</element>
        <element>quasibinomial(link="probit")</element>
        <element>quasibinomial(link="cloglog")</element>
        <element>quasibinomial(link="identity")</element>
        <element>quasibinomial(link="inverse")</element>
        <element>quasibinomial(link="log")</element>
        <element>quasibinomial(link="1/mu^2")</element>
        <element>quasibinomial(link="sqrt")</element>
        <element>quasipoisson</element>
        <element>quasipoisson(link="logit")</element>
        <element>quasipoisson(link="probit")</element>
        <element>quasipoisson(link="cloglog")</element>
        <element>quasipoisson(link="identity")</element>
        <element>quasipoisson(link="inverse")</element>
        <element>quasipoisson(link="log")</element>
        <element>quasipoisson(link="1/mu^2")</element>
        <element>quasipoisson(link="sqrt")</element>
      </values>
      <default>gaussian(link="identity")</default>
      <required>True</required>
    </field>
    <field name="subset" type="zope.schema.TextLine">
      <title>Subset</title>
      <description>Optional vector that may be used to specify a subset of the data. Please specify a column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="weights" type="zope.schema.TextLine">
      <title>Weights</title>
      <description>Optional vector that may be used to specify weights. Please specify a column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="na_action" type="zope.schema.Choice">
      <title>NA action</title>
      <description></description>
      <required>True</required>
      <default>na.fail</default>
      <vocabulary>lm_na_action_vocab</vocabulary>
    </field>
    <field name="start" type="zope.schema.TextLine">
      <title>Start</title>
      <description>Optional vector that may be used to specify start. Please specify a column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="eta_start" type="zope.schema.TextLine">
      <title>ETA start</title>
      <description>Optional vector that may be used to specify ETA start. Please specify a column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="mu_start" type="zope.schema.TextLine">
      <title>Mu start</title>
      <description>Optional vector that may be used to specify Mu start. Please specify a column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="offset" type="zope.schema.TextLine">
      <title>Offset</title>
      <description>Optional vector. Please specify a column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="method" type="zope.schema.Choice">
      <title>Method</title>
      <description></description>
      <values>
        <element>glm.fit</element>
        <element>model.frame</element>
      </values>
      <default>glm.fit</default>
      <required>True</required>
    </field>
    <field name="model" type="zope.schema.Bool" form:widget="z3c.form.browser.radio.RadioFieldWidget">
      <default>True</default>
      <title>Model returned</title>
      <required>False</required>
    </field>
    <field name="x" type="zope.schema.Bool" form:widget="z3c.form.browser.radio.RadioFieldWidget">
      <default>False</default>
      <title>x returned</title>
      <required>False</required>
    </field>
    <field name="y" type="zope.schema.Bool" form:widget="z3c.form.browser.radio.RadioFieldWidget">
      <default>True</default>
      <title>y returned</title>
      <required>False</required>
    </field>
    <field name="contrasts" type="zope.schema.TextLine">
      <title>Contrasts</title>
      <description>Optional list.</description>
      <required>False</required>
    </field>
  </schema>
</model>
