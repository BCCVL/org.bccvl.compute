<model xmlns:security="http://namespaces.plone.org/supermodel/security"
       xmlns:marshal="http://namespaces.plone.org/supermodel/marshal"
       xmlns:form="http://namespaces.plone.org/supermodel/form"
       xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema based-on="zope.interface.Interface">
    <field name="sigma_formula" type="zope.schema.TextLine">
      <title>Sigma formula</title>
      <description></description>
      <required>False</required>
      <default>~1</default>
    </field>
    <field name="nu_formula" type="zope.schema.TextLine">
      <title>Nu formula</title>
      <description></description>
      <required>False</required>
      <default>~1</default>
    </field>
    <field name="tau_formula" type="zope.schema.TextLine">
      <title>Tau formula</title>
      <description></description>
      <required>False</required>
      <default>~1</default>
    </field>
    <field name="family" type="zope.schema.TextLine">
      <title>Family</title>
      <description>Please refer to the R documentation</description>
      <required>False</required>
      <default>NO()</default>
    </field>
    <field name="weights" type="zope.schema.TextLine">
      <title>Weights</title>
      <description>Optional vector that may be used to specify weights. Please specify a column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="contrasts" type="zope.schema.TextLine">
      <title>Contrasts</title>
      <description>Optional list.</description>
      <required>False</required>
    </field>
    <field name="method" type="zope.schema.TextLine">
      <title>Method</title>
      <description>Please refer to the R documentation</description>
      <default>RS()</default>
      <required>False</required>
    </field>
    <field name="start_from" type="zope.schema.TextLine">
      <title>Start from</title>
      <description>Fitted values from a prior fitted GAMLSS model.</description>
      <required>False</required>
    </field>
    <field name="mu_start" type="zope.schema.TextLine">
      <title>Mu start</title>
      <description>Optional scalar or vector that may be used to specify Mu start. The vector could be column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="sigma_start" type="zope.schema.TextLine">
      <title>Sigma start</title>
      <description>Optional scalar or vector that may be used to specify Sigma start. The vector could be column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="nu_start" type="zope.schema.TextLine">
      <title>Nu start</title>
      <description>Optional scalar or vector that may be used to specify Nu start. The vector could be column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="tau_start" type="zope.schema.TextLine">
      <title>Tau start</title>
      <description>Optional scalar or vector that may be used to specify Tau start. The vector could be column included in the data model.</description>
      <required>False</required>
    </field>
    <field name="mu_fix" type="zope.schema.Bool" form:widget="z3c.form.browser.radio.RadioFieldWidget">
      <default>False</default>
      <title>Mu fix</title>
      <required>False</required>
    </field>
    <field name="sigma_fix" type="zope.schema.Bool" form:widget="z3c.form.browser.radio.RadioFieldWidget">
      <default>False</default>
      <title>Sigma fix</title>
      <required>False</required>
    </field>
    <field name="nu_fix" type="zope.schema.Bool" form:widget="z3c.form.browser.radio.RadioFieldWidget">
      <default>False</default>
      <title>Nu fix</title>
      <required>False</required>
    </field>
    <field name="tau_fix" type="zope.schema.Bool" form:widget="z3c.form.browser.radio.RadioFieldWidget">
      <default>False</default>
      <title>Tau fix</title>
      <required>False</required>
    </field>
    <field name="control" type="zope.schema.TextLine">
      <title>Control</title>
      <description></description>
      <required>False</required>
    </field>
    <field name="i_control" type="zope.schema.TextLine">
      <title>i control</title>
      <description></description>
      <required>False</required>
    </field>
    <field name="other_args" type="zope.schema.TextLine">
      <title>Other args</title>
      <description>This field enables an expert user to specify arguments that can be passed to lower level regression functions.</description>
      <required>False</required>
    </field>
    <field name="random_seed" type="zope.schema.Int">
      <description>random seed used</description>
      <required>False</required>
      <title>random seed</title>
      <min>-2147483648</min>
      <max>2147483647</max>      
    </field>    
  </schema>
</model>
